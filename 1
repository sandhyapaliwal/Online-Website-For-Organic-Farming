#include <stdio.h>
#include<unistd.h>
int main()
{ 
    int ar[5]={1,2,3,4,5}; 
    int evenSum=0;
    int oddSum=0; 
    int n=fork(); 
    if(n=0){ 
        for(int i=0;i<10;i++){  
            if(ar[i]%2!=0){ 
                oddSum=ar[i]+oddSum;
            }
        } 
    printf("oddSum is %d\n and n is %d\n ",oddSum,n); 
    } 
    else{ 
     for(int i=0;i<10;i++){  
            if(ar[i]%2==0){ 
                evenSum=ar[i]+evenSum;
            }
        } 
    printf("evenSum is %d\n and n is %d\n ",evenSum,n);    
    }

    return 0;
}
